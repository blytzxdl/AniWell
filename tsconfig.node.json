{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": ["electron.vite.config.*", "src/main/**/*", "src/preload/**/*", "src/server/**/*","plugins/**/*"],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "paths": {
      "@m/*": ["./src/main/*"],
      "@s/*": ["./src/server/*"],
  },
  }
}
